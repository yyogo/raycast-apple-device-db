{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "apple-device-db",
  "title": "Apple Device DB",
  "description": "Find and show details of Apple devices",
  "icon": "extension-icon.png",
  "author": "yyogo",
  "categories": [
    "Documentation"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "index",
      "title": "Apple devices",
      "subtitle": "List devices and SOCs",
      "description": "Show a list of Apple devices and SOCs",
      "mode": "view",
      "keywords": ["imodels", "idevices", "iphone", "ipad"]
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.53.4"
  },
  "devDependencies": {
    "@raycast/eslint-config": "1.0.5",
    "@types/node": "18.8.3",
    "@types/react": "18.0.9",
    "eslint": "^7.32.0",
    "prettier": "^2.5.1",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish"
  }
}